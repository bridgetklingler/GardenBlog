
@model IEnumerable<GardenBlog.Models.Post>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PostIndex</title>
</head>
<body>
    @foreach (var post in Model)
    {
        <h3>@post.Title</h3>
        <span>@post.Author.</span>
        <span>@post.Category.</span>
        <span>@post.TimeStamp</span>
        <p>@post.Body</p>
    }
</body>
</html>
