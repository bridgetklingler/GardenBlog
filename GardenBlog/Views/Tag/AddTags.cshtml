@model IEnumerable<GardenBlog.Models.Tag>
@{
    ViewData["Title"] = "AddTags";
    Layout = "_layout";
}

<h2>AddTags</h2>

<div>
    @foreach (var item in Model)
    {
        <div class="form-group">
            <input type="submit" value="@item.TagName" class="btn btn-default" />
        </div>
        <div class="form-group">
            <input type="hidden" value="@ViewBag.PostId" />
        </div>
    }
</div>

